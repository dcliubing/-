Sub 批注定位()
Dim rng As Range, a As Range
Dim fh, fl, lh, ll As Integer
fh = 1
fl = 1
lh = 58
ll = 33
For n = 1 To 3 Step 1

Set rng = Range(Cells(fh, fl), Cells(lh, ll))
For Each a In rng
If Not a.Comment Is Nothing Then

If a.Comment.Text Like "*位号*" Then


MsgBox a.Value

ElseIf a.Comment.Text Like "*数量*" Then

MsgBox a.Value

End If
End If
Next a

fh = fh + 58
lh = lh + 58


Next n





End Sub
